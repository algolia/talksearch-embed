{
  "name": "talksearch",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "serve": "preact build && preact serve",
    "dev": "preact watch --https",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": ["algolia/react"],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "eslintIgnore": ["build/*"],
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "eslint": "^4.12.1",
    "eslint-config-algolia": "^13.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.60.1",
    "if-env": "^1.0.0",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.0",
    "preact-cli-plugin-typescript": "^0.2.2",
    "prettier": "^1.9.0",
    "sass-loader": "^6.0.6",
    "tachyons-algolia": "^0.3.15",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "@haroenv/react-pinboard": "0.0.19",
    "@types/algoliasearch": "^3.24.4",
    "cross-env": "^5.1.1",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "react-instantsearch": "^4.2.0",
    "react-instantsearch-theme-algolia": "^4.3.0-beta.0",
    "react-pinboard": "^0.0.9",
    "react-youtube": "^7.5.0",
    "sec-to-min": "^1.0.1"
  }
}
